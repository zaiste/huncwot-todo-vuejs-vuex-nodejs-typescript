    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "sass-loader": "^6.0.7",
    "typescript": "^2.8.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  }
}
