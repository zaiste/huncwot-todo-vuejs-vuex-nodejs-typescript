  "license": "MIT",
  "private": true,
  "scripts": {
    "client": "webpack-dev-server --mode development --open --config config/webpack.dev.js",
    "build": "webpack --progress --hide-modules --config config/webpack.prod.js",
    "server": "huncwot server",
    "watch-ts": "tsc -w",
    "start": "concurrently -k -p \"[{name}]\" -n \"Client,TypeScript,Server\" -c \"magenta.bold,cyan.bold,blue.bold\" \"yarn run client\" \"yarn run watch-ts\" \"yarn run server\""
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.5.3",
    "axios": "^0.18.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "huncwot": "^0.20.0",
    "knex": "^0.14.5",
    "sqlite3": "^4.0.0",
    "validate": "^4.2.0",
    "vue": "^2.5.16",
    "vue-apollo": "^3.0.0-beta.5",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@types/node": "^9.6.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
